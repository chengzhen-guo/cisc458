using input, output;
var score = 0;

put ("Please input your score of the CISC/CMPE 458 course:"); putln;
get (score);

if (score >= 80) then
    put ("A - Good job, you are from team E right?"); putln;
elseif (score >= 70) then
    put ("B - It is acceptable."); putln;
elseif (score >= 60) then
    put ("C - It is OK."); putln;
elseif (score >= 50) then
    put ("D - Not good, but you can still graduate."); putln;
else
    put ("F - You cannot graduate."); putln;
end;