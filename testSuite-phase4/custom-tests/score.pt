using input, output;
var score = 0;

put ("Please input your score of the CISC/CMPE 458 course:"); putln;
get (score);

if (score > 90) then
    put ("A - Good job, you are from team E right?");
elseif (score > 80) then
    put ("B - It is acceptable.");
elseif (score > 70) then
    put ("C - It is OK.");
elseif (score > 60) then
    put ("D - What a s**ty work, but you can still graduate.");
else
    put ("F - You cannot graduate.");
end

putln;
getln;